{
  "name": "supplycart/money",
  "description": "Package to handle money operation for supplycart",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-intl": "*",
    "brick/money": "^0.8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.3.1",
    "orchestra/canvas": "^8.1.2"
  },
  "autoload": {
    "psr-4": {
      "Supplycart\\Money\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Supplycart\\Money\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Supplycart\\Money\\MoneyServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Azri Ismail",
      "email": "mazri.iar@gmail.com"
    },
    {
      "name": "Reno Looijmans",
      "email": "info@renolooijmans.nl"
    }
  ]
}
